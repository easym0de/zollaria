<div>
  <h1 style="text-align: center;">
    Your Inventory
  </h1>
</div>

<div class="hero-unit ">
<div class="row">
<div class="span3">
  <% @items[:items_col1].each do |item| %>
    <div class="well-home">
      <div>
      	  <% unless item[:medium_image] %>
            <%= link_to image_tag(item[:medium_image], :alt=> item[:title]), item[:detail_page_url], :target => "_blank"%>
		  <% end %>
		  <div>
            <%= item[:title] %>
    	  </div> 
      </div>
    </div>
  <% end%>
</div>
